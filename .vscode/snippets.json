{
  "export": {
    "prefix": "e",
    "body": "export $1"
  },

  "export default": {
    "prefix": "ed",
    "body": "export default $1"
  },

  "function": {
    "prefix": "f",
    "body": "const $1 = () => {$2}"
  },

  "function component": {
    "prefix": "fc",
    "body": [
      "const $1: React.FC = () => {",
      "  ",
      "",
      "  return <></>;",
      "};",
      "",
      "export default $1;"
    ]
  }
}
